package pattern;

import java.util.Scanner;

public class Pattern1 {
	public static void main(String[] args) {
		int n = new Scanner(System.in).nextInt();
		int i = 1;
		while(i<=n) {
			int j = 1;
			while(j<=i) {
				System.out.println(j);
				j=j+1;
				
			}
			i=i+1;
			System.out.println();
		}
	}

}
