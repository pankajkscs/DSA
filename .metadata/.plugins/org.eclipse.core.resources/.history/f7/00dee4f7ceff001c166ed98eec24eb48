package while_loop;

import java.util.Scanner;

public class SumOfNEvenNo {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int i =2;
		int sum =0;
		while(i<=n) {
			if(n%i==0) {
				sum =sum+i;
				i =i+1;
				
			}
			System.out.println(sum);
		}
	}

}
